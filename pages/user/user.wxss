.userInfo{display: flex; align-items: center;background: #fff;padding: 25rpx 0;box-shadow: 0px 2px 6px 0px rgba(0,0,0,0.10);}
.avatar{
  width: 120rpx;
  height: 120rpx;
  margin: 0 35rpx;
}

.username-wapper{flex: 1;}
.username{
  font-size: 32rpx;
  color: #333333;
  padding: 0 8rpx;
  position: relative;
}
.username::before{
  content: '';
  position: absolute;
  bottom: 0;
  left: 0;
  display: block;
  width: 100%;
  height: 16rpx;
  background: #FFDCCF;
  z-index: -1;
}
.questions{
  font-size: 20rpx;
  height: 28rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  margin: 0 45rpx;
  
}
.items-title{
  display: flex;
  margin: 23rpx 0;
  padding: 30rpx 0;
  font-size: 28rpx;
  color: #333;
  justify-content: space-around;
  background: #fff;
  box-shadow: 0px 2px 6px 0px rgba(0,0,0,0.10);
  position: relative;
}
.bar{
  position: absolute;
  width: 80rpx;
  height: 8rpx;
  border-radius: 4rpx;
  bottom: 0px;
  left: 50rpx;
 background: linear-gradient(to right, rgba(254,153,111,1) 0%, rgba(246,112,90,1) 100%);
}
